Public Class Form1
    Dim src As String = ""
    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        Dim x As Integer = src.Length
        Dim maxTotal As Long = 1
        For i As Integer = 0 To x - 1 - 13
            Dim total As Long = 1
            For j As Integer = 0 To 12
                'MsgBox(Convert.ToInt32(src(i + j)) - 48)
                total = total * (Convert.ToInt64(src(i + j)) - 48)
            Next
            If total > maxTotal Then
                maxTotal = total
            End If
        Next
        MsgBox(maxTotal)
    End Sub
End Class
